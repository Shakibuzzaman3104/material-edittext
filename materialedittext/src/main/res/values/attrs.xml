<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MaterialEditText">
        <attr name="isRequired" format="boolean" />
        <attr name="isEnabled" format="boolean" />
        <attr name="hint" format="string" />
        <attr name="isReadOnly" format="boolean" />
        <attr name="radius" format="float" />
        <attr name="inputType" format="enum">
            <enum name="digit" value="0"/>
            <enum name="name" value="1"/>
            <enum name="address" value="2"/>
            <enum name="email" value="3"/>
            <enum name="phone" value="4"/>
            <enum name="password" value="5"/>
        </attr>
    </declare-styleable>
</resources>